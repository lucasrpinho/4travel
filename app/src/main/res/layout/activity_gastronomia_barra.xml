        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMaccheroneAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMaccheroneNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <Button
            android:layout_width="wrap_content"
            android:id="@+id/nextScreen2"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_marginTop="40sp"
            android:layout_gravity="center"
            android:textSize="24sp"
            android:backgroundTint="#174280"
            android:text="AVANÇAR"/>




    </LinearLayout>

</ScrollView>