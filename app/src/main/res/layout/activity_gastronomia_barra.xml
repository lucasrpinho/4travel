            android:layout_width="35sp"
            android:id="@+id/btnBeccoNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/deliriotropical"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/delirio"
            android:layout_marginStart="145sp"
            android:layout_marginTop="-133sp"
            android:text="@string/delirio"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/deliriodescricao"/>

