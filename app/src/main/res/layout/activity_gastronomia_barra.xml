            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/maccherone"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:id="@+id/maccherone"
            android:layout_marginTop="-133sp"
            android:text="@string/maccherone"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/maccheronedescricao"/>

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMaccheroneManha"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
