<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.gastronomiaGuaratiba"
    android:background="@drawable/travelbackgroundpng2"
    android:fillViewport="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:isScrollContainer="true">

        <TextView
            android:id="@+id/gastronomiaRecreio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="17dp"
            android:text="@string/gastronomiaRecreio"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="36sp" />

        <ImageView
            android:layout_width="130dp"
            android:layout_marginStart="10dp"
            android:layout_height="130dp"
            android:layout_marginTop="50sp"
            android:background="@drawable/alegriaguaratiba"
            android:contentDescription="@string/todo" />

        <TextView
            android:id="@+id/alegria"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145dp"
            android:layout_marginTop="-133dp"
            android:text="@string/alegria"
            android:textColor="@color/black"
            android:textSize="22sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145dp"
            android:textColor="@color/black"
            android:text="@string/alegriadescricao" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnAlegriaManha"
            android:layout_height="35sp"
            android:layout_marginStart="165sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnAlegriaTarde"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="230sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnAlegriaNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130dp"
            android:layout_marginStart="10dp"
            android:layout_height="130dp"
            android:layout_marginTop="50sp"
            android:background="@drawable/biradeguaratiba"
            android:contentDescription="@string/todo" />

        <TextView
            android:id="@+id/bira"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145dp"
            android:layout_marginTop="-133dp"
            android:text="@string/bira"
            android:textColor="@color/black"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145dp"
            android:textColor="@color/black"
            android:text="@string/biradescricao" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnBira"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnBiraAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnBiraNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/tiapenha"/>

        <TextView
            android:layout_width="match_parent"
            android:id="@+id/tiapenha"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:layout_marginTop="-133sp"
            android:text="@string/tiapenha"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/tiapenhadescricao"/>

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTia"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTiaAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTiaNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/mirante"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/mirante"
            android:layout_marginStart="145sp"
            android:layout_marginTop="-133sp"
            android:text="@string/mirante"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/mirantedescricao"/>

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMirante"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMiranteAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnMiranteNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/aquarios"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:layout_marginTop="-133sp"
            android:text="@string/aquarios"
            android:id="@+id/aquarios"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/aquariosdescricao"/>

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnaquarios"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnAquariosAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnAquariosNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <ImageView
            android:layout_width="130sp"
            android:layout_height="130sp"
            android:layout_marginStart="10sp"
            android:layout_marginTop="50sp"
            android:background="@drawable/tropicana"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:layout_marginTop="-133sp"
            android:id="@+id/tropicana"
            android:text="@string/tropicana"
            android:textColor="@color/black"
            android:textSize="22sp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="145sp"
            android:textColor="@color/black"
            android:text="@string/tropicanadescricao"/>

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTropicana"
            android:layout_height="35sp"
            android:visibility="invisible"
            android:layout_marginStart="130sp"
            android:layout_marginTop="10sp"
            android:layout_weight="1"
            android:background="@drawable/matinal" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTropicanaAlmoço"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="200sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/vespertino" />

        <ImageButton
            android:layout_width="35sp"
            android:id="@+id/btnTropicanaNoite"
            android:layout_height="35sp"
            android:layout_weight="1"
            android:layout_marginStart="300sp"
            android:layout_marginTop="-35sp"
            android:background="@drawable/noturno" />

        <Button
        android:layout_width="wrap_content"
        android:id="@+id/nextScreen6"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_marginTop="40sp"
        android:layout_gravity="center"
        android:textSize="24sp"
        android:backgroundTint="#174280"
        android:text="PRÓXIMA TELA"/>

    </LinearLayout>

</ScrollView>